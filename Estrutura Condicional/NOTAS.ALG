Algoritmo "notas"

Var

   nota1, nota2, final : real

Inicio

   escreva("Digite a primeira nota: ")
   leia(nota1)
   escreva("Digite a segunda nota: ")
   leia(nota2)
   
   final<- nota1 + nota2
   
   escreval("NOTA FINAL = ", final)
   
   se final < 60 entao
      escreva("REPROVADO")
   fimse

   

Fimalgoritmo